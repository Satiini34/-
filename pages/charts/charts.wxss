}
.container {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 320rpx;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
