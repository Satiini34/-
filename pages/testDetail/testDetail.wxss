page {
  width: 100%;
  height: 100%;
  overflow-x: hidden;
  background: rgb(236, 236, 236);
}
.heaerImg {
  width: 100%;
  height: 366rpx;
}
.title {
  width: 100%;
  padding-top: 50rpx;
  font-size: 36rpx;
  color: #333;
  text-align: center;
  background: #fff;
}
.contnet {
  width: 92%;
  padding: 50rpx 4%;
  background: #fff;
  color: #666;
  font-size: 24rpx;
  line-height: 36rpx;
}
.bottom {
  width: 92%;
  padding: 0 4%;
  background: #fff;
  height: 50rpx;
  padding-bottom: 40rpx;
}
.textBox_bottom1 {
  height: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  background: #fff;
}
.testPart {
  width: 92%;
  padding: 0 4% 30rpx 4%;
  text-align: center;
  background: #fff;
  height: 88rpx;
}
.testBtn {
  height: 100%;
  line-height: 88rpx;
  background: #37b7fa;
  color: #fff;
  font-size: 30rpx;
  font-weight: bold;
  border-radius: 40rpx;
  text-align: center;
}
.testNotice {
  width: 92%;
  position: relative;
  left: 4%;
  background: transparent;
  margin-top: 40rpx;
}
.testNotice_title {
  color: #333;
  font-size: 26rpx;
  margin-left: 20rpx;
}
.notice_content {
  margin-top: 30rpx;
  font-size: 26rpx;
  line-height: 50rpx;
  color: #666;
}
.contact {
  color: rgb(55, 183, 250);
  display: inline;
  padding: 0;
  background: transparent;
  font-size: 26rpx;
  border: none;
}
.contact::after {
  width: 0;
  height: 0;
  border: none;
}